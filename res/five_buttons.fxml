<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane fx:id="pane" xmlns:fx="http://javafx.com/fxml"
          fx:controller="de.hhu.propra16.FiveButtonsController"
          alignment="CENTER"
          hgap="10"
          vgap="10">
    <padding><Insets top="25" right="25" bottom="10" left="25"/></padding>

    <Text text="Five Buttons" GridPane.columnIndex="0" GridPane.rowIndex="0"/>

    <Button text="Open File" GridPane.columnIndex="1" GridPane.rowIndex="0" onAction="#handleFileButton" />

    <Label id="fileLabel" text="" GridPane.columnIndex="1" GridPane.rowIndex="1" />

</GridPane>